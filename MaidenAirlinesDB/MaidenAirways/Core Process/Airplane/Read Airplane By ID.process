<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.tibco.com/schemas/MaidenAirways/Schemas/Schema.xsd" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:pfx2="http://www.tibco.com/schemas/MaidenAirways/Schemas/Schema.xsd2" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <xsd:import namespace="http://www.tibco.com/schemas/MaidenAirways/Schemas/Schema.xsd" schemaLocation="/Schemas/Airports.xsd"/>
    <xsd:import namespace="http://www.tibco.com/schemas/MaidenAirways/Schemas/Schema.xsd2" schemaLocation="/Schemas/Airplanes.xsd"/>
    <pd:name>Core Process/Airplane/Read Airplane By ID.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx2:Airplane"/>
    <pd:startX>150</pd:startX>
    <pd:startY>64</pd:startY>
    <pd:returnBindings>
        <xsl:choose>
            <xsl:when test="$Query-By-ID/resultSet/Record">
                <pfx2:Airplanes>
                    <xsl:for-each select="$Query-By-ID/resultSet/Record">
                        <pfx2:Airplane>
                            <xsl:attribute name="ID">
                                <xsl:value-of select="id"/>
                            </xsl:attribute>
                            <xsl:attribute name="Model">
                                <xsl:value-of select="model"/>
                            </xsl:attribute>
                            <xsl:attribute name="Cargo_Hold_Capacity">
                                <xsl:value-of select="cargo_hold_capacity"/>
                            </xsl:attribute>
                            <xsl:attribute name="Number_Seats">
                                <xsl:value-of select="number_seats"/>
                            </xsl:attribute>
                        </pfx2:Airplane>
                    </xsl:for-each>
                </pfx2:Airplanes>
            </xsl:when>
            <xsl:otherwise>
                <pfx2:Airplanes>
                    <pfx2:Airplane>
                        <xsl:attribute name="ID">
                            <xsl:value-of select="&quot;&quot;"/>
                        </xsl:attribute>
                        <xsl:attribute name="Model">
                            <xsl:value-of select="&quot;&quot;"/>
                        </xsl:attribute>
                        <xsl:attribute name="Cargo_Hold_Capacity">
                            <xsl:value-of select="&quot;&quot;"/>
                        </xsl:attribute>
                        <xsl:attribute name="Number_Seats">
                            <xsl:value-of select="&quot;&quot;"/>
                        </xsl:attribute>
                    </pfx2:Airplane>
                </pfx2:Airplanes>
            </xsl:otherwise>
        </xsl:choose>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx2:Airplanes"/>
    <pd:endX>440</pd:endX>
    <pd:endY>261</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables>
        <Status>
            <xsd:element name="Status">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="value" type="xsd:string"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </Status>
    </pd:processVariables>
    <pd:targetNamespace>http://xmlns.example.com/1565970135118</pd:targetNamespace>
    <pd:activity name="Query By ID">
        <pd:type>com.tibco.plugin.jdbc.JDBCQueryActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCQueryActivity</pd:resourceType>
        <pd:x>287</pd:x>
        <pd:y>63</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <maxRows>100</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/Connections/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <statement>SELECT * FROM Airplane WHERE ID = ?</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>ID</parameterName>
                    <dataType>INTEGER</dataType>
                </parameter>
            </Prepared_Param_DataType>
            <oraObjects/>
            <oraTables/>
            <QueryOutputCachedSchemaColumns>id</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>4</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>INT</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>model</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>12</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>VARCHAR</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>cargo_hold_capacity</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>4</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>INT</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
            <QueryOutputCachedSchemaColumns>number_seats</QueryOutputCachedSchemaColumns>
            <QueryOutputCachedSchemaDataTypes>4</QueryOutputCachedSchemaDataTypes>
            <QueryOutputCachedSchemaDataTypesName>INT</QueryOutputCachedSchemaDataTypesName>
            <QueryOutputCachedSchemaStatus>RequiredElement</QueryOutputCachedSchemaStatus>
        </config>
        <pd:inputBindings>
            <jdbcQueryActivityInput>
                <ID>
                    <xsl:choose>
                        <xsl:when test="exists($Start/pfx2:Airplane/@ID)">
                            <xsl:value-of select="$Start/pfx2:Airplane/@ID"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </ID>
            </jdbcQueryActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Null">
        <pd:type>com.tibco.plugin.timer.NullActivity</pd:type>
        <pd:resourceType>ae.activities.null</pd:resourceType>
        <pd:x>285</pd:x>
        <pd:y>260</pd:y>
        <config/>
        <pd:inputBindings/>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Query By ID</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Query By ID</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Query By ID</pd:from>
        <pd:to>Null</pd:to>
        <pd:xpathDescription>Error</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>error</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Null</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>